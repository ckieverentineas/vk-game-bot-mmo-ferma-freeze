{
  "name": "typescript-node-starter",
  "version": "1.0.0",
  "description": "Typescript",
  "scripts": {
    "start": "ts-node-dev --respawn src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "eslint-check": "npx eslint src/index.ts",
    "eslint-fix": "npx eslint src/index.ts --fix",
    "prettier-start": "npx prettier --single-quote --tab-width 2 src/index.ts",
    "prettier-clean": "npx prettier src/index.ts",
    "prettier-write": "prettierrc --write src/index.ts"
  },
  "keywords": [
    "typescript",
    "tsc",
    "javascript",
    "NodeJS",
    "starter",
    "kit"
  ],
  "author": "Yassine",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pixelpromotions/typescript-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/Pixelpromotions/typescript-starter-kit/issues"
  },
  "homepage": "https://github.com/Pixelpromotions/typescript-starter-kit#readme",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@vk-io/hear": "^1.1.1",
    "dotenv": "^16.1.4",
    "prisma": "^4.15.0",
    "vk-io": "^4.8.3",
    "vk-io-question": "^3.1.1"
  }
}
